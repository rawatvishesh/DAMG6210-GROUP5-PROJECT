---------Warehouse----------
create table Warehouse (  
  Warehouse_id           number not null,  
  Warehouse_name         varchar2(25) not null,
  Address                varchar2(100) not null,
  City                   varchar2(50) not null,
  States                 varchar2(50) not null,
  Country                varchar2(50) not null,
  Zip_code               number not null,
  constraint pk_warehouse_id primary key (Warehouse_id)
);

Insert into Warehouse(Warehouse_id, Warehouse_name, Address, City, States, Country, Zip_code) values (1,'Warehouse1','2 Temont Street ', 'Boston', 'Massachusetts', 'United States of America', '02124');
