-------Order Status------------
create table Order_status (  
  Status_id           number not null,  
  Status              varchar2(25) not null,  
  constraint pk_status_id primary key (Status_id)
);

Insert into Order_status (Status_id, Status) values (1, 'Order placed');
Insert into Order_status (Status_id, Status) values (2, 'Shipped');
Insert into Order_status (Status_id, Status) values (3, 'On the way');
Insert into Order_status (Status_id, Status) values (4, 'Delivered');



Drop table Country cascade constraints;
Drop table States cascade constraints;
Drop table City cascade constraints;
drop table Customers cascade constraints;
Drop table Address cascade constraints;
Drop table Card_detail cascade constraints;
Drop table Subscription_detail cascade constraints;
Drop table Customer_membership cascade constraints;
Drop table Warehouse cascade constraints;
Drop table Product_category cascade constraints;
Drop table Product cascade constraints;
Drop table Order_status cascade constraints;

------Orders---------------

create table Orders (  
  Order_id            number not null,  
  Customer_id         number not null,  
  Order_date          date not null,  
  Shipped_date        date not null,
  Status_id           number not null,
  Order_total         float not null,
  Discount_total      float not null,
  Transaction_mode    varchar2(25) not null,
  constraint pk_order_id primary key (Order_id),
  constraint fk_customer_id foreign key (Customer_id)
  references Customers (customer_id),
  constraint fk_statusid foreign key (Status_id)
  references Order_status  (Status_id)
);

-----Order Product------

create table Orders_product (  
  Order_product_id    number not null,  
  Order_id            number not null,  
  Product_id          number not null,  
  Order_quantity      number not null,
  Unique_price        float not null,
  constraint pk_order_product_id primary key (Order_product_id),
  constraint fk_order_id foreign key (Order_id)
  references Orders (Order_id),
  constraint fk_product1 foreign key (Product_id)
  references Product (Product_id)
);









Drop table Country cascade constraints;
Drop table States cascade constraints;
Drop table City cascade constraints;
drop table Customers cascade constraints;
Drop table Address cascade constraints;
Drop table Card_detail cascade constraints;
Drop table Subscription_detail cascade constraints;
Drop table Customer_membership cascade constraints;
Drop table Warehouse cascade constraints;
Drop table Product_category cascade constraints;
Drop table Product cascade constraints;
Drop table Order_status cascade constraints;
Drop table Orders cascade constraints;
Drop table Orders_product cascade constraints;
